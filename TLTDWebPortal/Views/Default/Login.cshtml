@model TLTDWebPortal.Models.LoginParam

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>


@using (Html.BeginForm())
{
    <div class="form-group">
        <p>Username @Html.TextBoxFor(m => m.username)</p>
    </div>
    <div class="form-group">
        <p>Password @Html.TextBoxFor(m => m.password, new { type = "password" })</p>
    </div>

    <input type="submit" value="Log-in" />
}


